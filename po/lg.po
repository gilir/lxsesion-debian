#, c-format
msgid "Error: %s\n"
msgstr "Kiremya: %s\n"

# Message telling you that there is no LXSession running at the moment.
#: ../lxsession-logout/lxsession-logout.c:388
msgid "LXSession is not running."
msgstr "LXSession tebindabinda."

#: ../lxsession-logout/lxsession-logout.c:564
#, c-format
msgid "<b><big>Logout %s session?</big></b>"
msgstr "<b><big>Olutuula lwa %s lukome?</big></b>"

# Turn the machine completely off
#: ../lxsession-logout/lxsession-logout.c:572
msgid "Sh_utdown"
msgstr "_Gyako kompyuta"

# Make the machine shut down and restart itself
#: ../lxsession-logout/lxsession-logout.c:583
msgid "_Reboot"
msgstr "_Damu-okukoleeza kompyuta"

# Have the machine pause, leaving things exactly where they are
#: ../lxsession-logout/lxsession-logout.c:594
msgid "_Suspend"
msgstr "Kompyuta gi_yimirizemu"

# Have the machine close all jobs and go into a low energy state - still logged in
#: ../lxsession-logout/lxsession-logout.c:605
msgid "_Hibernate"
msgstr "_Kompyuta gyebase"

# Work in another user account while keeping the current one open and read to return to
#: ../lxsession-logout/lxsession-logout.c:616
msgid "S_witch User"
msgstr "Kolera mu akawunti e_ndala"

#. Create the Logout button.
#: ../lxsession-logout/lxsession-logout.c:625
msgid "_Logout"
msgstr "Komya _olutuula luno"

